/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=inverseA);
    Not(in=b , out=inverseB);
    Nand(a=inverseA , b=b , out=nandBInverseA);
    Nand(a=a , b=inverseB , out=nandAInverseB);
    Nand(a=nandBInverseA, b=nandAInverseB, out=out);
}
